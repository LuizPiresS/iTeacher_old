{
  "name": "iteacher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write **/*.{json,ts}",
    "lint": "eslint **/*.ts",
    "prebuild": "rm -rf build",
    "build": "tsc",
    "start:api": "node build/api/main.js",
    "start:api:watch": "nodemon -w build build/api/main.js",
    "test": "TZ=UTC jest",
    "coverage": "TZ=UTC jest --coverage",
    "db:migration:create": "typeorm migration:create",
    "db:migration:run": "typeorm migration:run",
    "db:migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.7",
    "fast-xml-parser": "^3.17.2",
    "fastify": "^2.14.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "pino-pretty": "^4.0.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  }
}
